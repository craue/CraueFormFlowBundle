{
	"name": "craue/formflow-bundle",
	"type": "symfony-bundle",
	"description": "Multi-step forms for your Symfony2 project.",
	"keywords": ["symfony2", "form"],
	"homepage": "https://github.com/craue/CraueFormFlowBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Raue",
			"email": "christian.raue@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://github.com/craue/CraueFormFlowBundle/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/form": "~2.3",
		"symfony/http-kernel": "~2.3",
		"symfony/translation": "~2.3",
		"symfony/yaml": "~2.3"
	},
	"require-dev": {
		"doctrine/common": "~2.3",
		"phpunit/phpunit": "~4.1",
		"sensio/framework-extra-bundle": "~2.1|~3.0",
		"symfony/symfony": "~2.3"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-0": {
			"Craue\\FormFlowBundle\\": ""
		}
	},
	"target-dir": "Craue/FormFlowBundle",
	"extra": {
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	}
}
